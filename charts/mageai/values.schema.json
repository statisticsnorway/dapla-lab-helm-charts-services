{
    "$schema": "http://json-schema.org/schema#",
    "type": "object",
    "properties": {
        "affinity": {
            "type": "object"
        },
        "config": {
            "type": "object"
        },
        "customLivenessProbe": {
            "type": "object"
        },
        "customReadinessProbe": {
            "type": "object"
        },
        "existingSecret": {
            "type": "string"
        },
        "extraEnvs": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "name": {
                        "type": "string"
                    },
                    "valueFrom": {
                        "type": "object",
                        "properties": {
                            "fieldRef": {
                                "type": "object",
                                "properties": {
                                    "fieldPath": {
                                        "type": "string"
                                    }
                                }
                            }
                        }
                    }
                }
            }
        },
        "extraVolumeMounts": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "mountPath": {
                        "type": "string"
                    },
                    "name": {
                        "type": "string"
                    }
                }
            }
        },
        "extraVolumes": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "name": {
                        "type": "string"
                    },
                    "persistentVolumeClaim": {
                        "type": "object",
                        "properties": {
                            "claimName": {
                                "type": "string"
                            }
                        }
                    }
                }
            }
        },
        "fullnameOverride": {
            "type": "string"
        },
        "image": {
            "type": "object",
            "properties": {
                "pullPolicy": {
                    "type": "string"
                },
                "repository": {
                    "type": "string"
                }
            }
        },
        "imagePullSecrets": {
            "type": "array"
        },
        "ingress": {
            "type": "object",
            "properties": {
                "annotations": {
                    "type": "object"
                },
                "className": {
                    "type": "string"
                },
                "enabled": {
                    "type": "boolean"
                },
                "hosts": {
                    "type": "array",
                    "items": {
                        "type": "object",
                        "properties": {
                            "host": {
                                "type": "string"
                            },
                            "paths": {
                                "type": "array",
                                "items": {
                                    "type": "object",
                                    "properties": {
                                        "path": {
                                            "type": "string"
                                        },
                                        "pathType": {
                                            "type": "string"
                                        }
                                    }
                                }
                            }
                        }
                    }
                },
                "tls": {
                    "type": "array"
                }
            }
        },
        "livenessProbe": {
            "type": "object",
            "properties": {
                "enabled": {
                    "type": "boolean"
                },
                "initialDelaySeconds": {
                    "type": "integer"
                },
                "path": {
                    "type": "string"
                },
                "port": {
                    "type": "string"
                },
                "timeoutSeconds": {
                    "type": "integer"
                }
            }
        },
        "nameOverride": {
            "type": "string"
        },
        "nodeSelector": {
            "type": "object"
        },
        "podAnnotations": {
            "type": "object"
        },
        "podSecurityContext": {
            "type": "object"
        },
        "readinessProbe": {
            "type": "object",
            "properties": {
                "enabled": {
                    "type": "boolean"
                },
                "initialDelaySeconds": {
                    "type": "integer"
                },
                "path": {
                    "type": "string"
                },
                "port": {
                    "type": "string"
                },
                "timeoutSeconds": {
                    "type": "integer"
                }
            }
        },
        "redis": {
            "type": "object",
            "properties": {
                "architecture": {
                    "type": "string"
                },
                "auth": {
                    "type": "object",
                    "properties": {
                        "enabled": {
                            "type": "boolean"
                        }
                    }
                },
                "customRedisURL": {
                    "type": "string"
                },
                "enabled": {
                    "type": "boolean"
                }
            }
        },
        "replicaCount": {
            "type": "integer"
        },
        "resources": {
            "type": "object"
        },
        "scheduler": {
            "type": "object",
            "properties": {
                "name": {
                    "type": "string"
                },
                "replicaCount": {
                    "type": "integer"
                }
            }
        },
        "secrets": {
            "type": "object"
        },
        "securityContext": {
            "type": "object"
        },
        "service": {
            "type": "object",
            "properties": {
                "annotations": {
                    "type": "object"
                },
                "port": {
                    "type": "integer"
                },
                "type": {
                    "type": "string"
                }
            }
        },
        "serviceAccount": {
            "type": "object",
            "properties": {
                "annotations": {
                    "type": "object"
                },
                "create": {
                    "type": "boolean"
                },
                "name": {
                    "type": "string"
                }
            }
        },
        "standaloneScheduler": {
            "type": "boolean"
        },
        "tolerations": {
            "type": "array"
        },
        "webServer": {
            "type": "object",
            "properties": {
                "name": {
                    "type": "string"
                },
                "replicaCount": {
                    "type": "integer"
                }
            }
        }
    }
}
